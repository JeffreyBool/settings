{
  "name": "jeffreybool/settings",
  "keywords": ["settings", "persistent settings", "laravel"],
  "description": "Persistent settings package for Laravel framework.",
  "authors": [
    {
      "name": "JeffreyBool",
      "email": "1402992668@qq.com"
    }
  ],

  "extra": {
    "laravel": {
      "providers": [
        "JeffreyBool\\Settings\\Providers\\SettingsServiceProvider"
      ],
      "aliases": {
        "Settings": "JeffreyBool\\Settings\\Facades\\Settings"
      }
    }
  },

  "license": "MIT",
  "require": {
    "php" : "^7.2",
    "illuminate/support": "^6.0|^7.0",
    "illuminate/console": "^6.0|^7.0",
    "illuminate/database": "^6.0|^7.0",
    "illuminate/filesystem": "^6.0|^7.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0|1.3.0",
    "phpunit/phpunit": "^8.0|^9.0"
  },
  "autoload": {
    "psr-0": {
      "JeffreyBool\\Settings\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "minimum-stability": "dev"
}
